{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "script-src 'self' 'unsafe-eval' https://telegram.org; object-src 'none';"
        },
        {
          "key": "Cache-Control",
          "value": "no-store, max-age=0"
        }
      ]
    }
  ],
  "routes": [
    { "src": "/index.html", "headers": { "Cache-Control": "no-store" } },
    { "src": "/shop.html", "dest": "/shop.html" }
  ]
}