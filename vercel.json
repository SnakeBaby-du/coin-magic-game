{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "script-src 'self' 'unsafe-eval' https://telegram.org https://telegram-web.app; object-src 'none'; frame-src 'none';"
        },
        {
          "key": "Cache-Control",
          "value": "no-store, no-cache, must-revalidate"
        }
      ]
    }
  ],
  "routes": [
    { 
      "src": "/index.html",
      "headers": {
        "Cache-Control": "no-store",
        "Content-Security-Policy": "script-src 'self' 'unsafe-eval' https://telegram.org https://telegram-web.app; object-src 'none'"
      }
    },
    { "src": "/shop.html", "dest": "/shop.html" },
    { "src": "/", "dest": "/index.html" }
  ]
}